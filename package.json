    "react-bootstrap": "^2.5.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.23.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "eslint": "8.23.0",
    "eslint-config-next": "12.3.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "typescript": "^4.4.4"
  }
